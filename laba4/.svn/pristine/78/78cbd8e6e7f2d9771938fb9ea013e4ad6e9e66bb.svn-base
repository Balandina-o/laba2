package laba4;

public class Square extends Figure{
    private int value;
    private double area;
    Square(int value){
        super.setName("квадрат");
        super.setStoron(4);
        this.value=value;
    }
    @Override
    public int Perimeter(){
        int per=4*value;
        return per;
    }

    @Override
    public double Area(){
        area=value*value;
        return area;
    }
}
